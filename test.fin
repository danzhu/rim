def List =
    Cons head tail
    Null

def Res =
    K value
    E error

def head lst = which lst of
    Cons a _ | K a
    _        | E ()

def unwrap (K value) = value

def id x = x

def main =
    let test =
        rim:version -> v
        rim:print v

    let lst = Cons test Null
    unwrap (head lst)
    id
        rim:print lst
